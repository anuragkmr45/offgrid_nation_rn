{
  "cli": {
    "version": ">= 16.13.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev",
      "env": {
        "ENVFILE": ".env.development"
      }
    },
    "preview": {
      "extends": "production",
      "channel": "qa",
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "ENVFILE": ".env.preview"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "environment": "production",
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "ios": {
        "simulator": false
      },
      "env": {"ENVFILE": ".env.production"}
    }
  },
  "submit": {
    "production": {},
    "preview": {}
  }
}