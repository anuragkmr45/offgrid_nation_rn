{
  "cli": {
    "version": ">= 16.13.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "store"
    },
    "preview": {
      "distribution": "store",
      "autoIncrement": true
    },
    "production": {
      "distribution": "store",
      "environment": "production",
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      },
      "env": {
        "APP_USERNAME": "APP_USERNAME",
        "APP_PASSWORD": "APP_PASSWORD"
      }
    }
  },
  "submit": {
    "production": {},
    "preview": {}
  }
}